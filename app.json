{
      "name": "Rename Bot",
      "description": "A Telegram Rename Bot.",
      "keywords": [
         "telegram",
         "bot",
         "rename",
         "retrogini"
      ],
   "repository": "https://github.com/Kazama009/Rename-Bot-Retrogini",
   "addons": [
      {
         "options": {
            "version": "13"
         },
         "plan": "heroku-postgresql"
      }
   ],
   "env": {
      "TOKEN": {
         "description": "Your bot token. Get one from @BotFather duh",
         "required": true,
         "value": ""
      },
      "API_ID": {
         "description": "Get API_ID from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
      "API_HASH": {
         "description": "Get API_HASH from my.telegram.org, used for telethon based modules.",
         "required": true,
         "value": ""
      },
